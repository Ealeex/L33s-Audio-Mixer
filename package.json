{
  "name": "l33s-audio-mixer",
  "version": "1.0.1",
  "description": "Simple audio equalizer with built in reverb slider.\nThe reverb slider is meant to simulate a car's interior.",
  "main": "inject.js",
  "scripts": {
    "build:content_scripts": "esbuild src/inject.ts --bundle --platform=node --format=iife --outfile=extension/content_scripts/inject.js",
    "build:popup": "esbuild src/popup.ts --bundle --platform=node --format=iife --outfile=extension/popup/popup.js",
    "build:options": "esbuild src/options.ts --bundle --platform=node --format=iife --outfile=extension/config_page/options.js",
    "build": "npm run build:content_scripts && npm run build:popup && npm run build:options",
    "start": "web-ext run -s extension --firefox-profile Testing --url https://www.youtube.com/watch?v=0NDqYZVbpho",
    "build-ext": "web-ext build -s extension",
    "sign": "web-ext sign --channel=listed -s extension --amo-metadata=extension/metadata.json --api-key=user:18163912:496 --api-secret=99f7bdc58abdcd7ade38bba73394cdebf0f4da16f4c53b2f1a98710b05a50c04"
  },
  "keywords": [
    "photos-music-videos",
    "tabs"
  ],
  "author": "Ealeex",
  "license": "MPL-2.0",
  "devDependencies": {
    "@types/chrome": "^0.0.326",
    "@types/firefox-webext-browser": "^120.0.4"
  },
  "dependencies": {
    "express": "^5.1.0",
    "webextension-polyfill": "^0.12.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Ealeex/L33s-Audio-Mixer"
  }
}
